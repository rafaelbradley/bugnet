    <add key="BugNetPassword" value="" />
    <add key="IssueIdRegEx" value="\[?([A-Za-z]{1,50}-(\d+))\]?" />
    <add key="BugNetServicesUrl" value="http://localhost:59847/WebServices/BugNetServices.asmx" />
